algoritmo "Somador"
var
   somador, contador, valor: Inteiro
inicio

   contador <- 1
   valor <- 0
   somador <- 0
   Enquanto contador <= 10 faca
      Escreva("Digite o", contador , " Numero: ")
      Leia(valor)
      contador <- contador + 1
      somador <- somador + valor
   fimenquanto
fimalgoritmo